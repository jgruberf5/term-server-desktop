chpasswd:
  expire: false
  list: |
    demo:DEMOPASSWORD
write_files:
- path: /etc/xrdp/startwm.sh
  permissions: '0755'
  content: |
    #!/bin/sh
    if test -r /etc/profile; then
      . /etc/profile
    fi
    if test -r /etc/default/locale; then
      . /etc/default/locale
      test -z "${LANG+x}" || export LANG
      test -z "${LANGUAGE+x}" || export LANGUAGE
      test -z "${LC_ADDRESS+x}" || export LC_ADDRESS
      test -z "${LC_ALL+x}" || export LC_ALL
      test -z "${LC_COLLATE+x}" || export LC_COLLATE
      test -z "${LC_CTYPE+x}" || export LC_CTYPE
      test -z "${LC_IDENTIFICATION+x}" || export LC_IDENTIFICATION
      test -z "${LC_MEASUREMENT+x}" || export LC_MEASUREMENT
      test -z "${LC_MESSAGES+x}" || export LC_MESSAGES
      test -z "${LC_MONETARY+x}" || export LC_MONETARY
      test -z "${LC_NAME+x}" || export LC_NAME
      test -z "${LC_NUMERIC+x}" || export LC_NUMERIC
      test -z "${LC_PAPER+x}" || export LC_PAPER
      test -z "${LC_TELEPHONE+x}" || export LC_TELEPHONE
      test -z "${LC_TIME+x}" || export LC_TIME
      test -z "${LOCPATH+x}" || export LOCPATH
    fi
    if test -r /etc/profile; then
      . /etc/profile
    fi
    startxfce4
- path: /usr/bin/xrdp-reset-certs
  permissions: '0755'
  content: |
    #!/bin/bash
    # force delete
    rm -rf /etc/xrdp/cert.pem /etc/xrdp/key.pem /etc/xrdp/rsakeys.ini
    # generate xrdp key
    if [ ! -f "/etc/xrdp/rsakeys.ini" ]; then
      xrdp-keygen xrdp auto
    fi
    # generate certificate for tls connection
    if [ ! -f "/etc/xrdp/cert.pem" ];
    then
        # delete eventual leftover private key
        rm -f /etc/xrdp/key.pem || true
        cd /etc/xrdp
        if [ ! $CERTIFICATE_SUBJECT ]; then
            CERTIFICATE_SUBJECT="/C=US/ST=Some State/L=Some City/O=Some Org/OU=Some Unit/CN=Terminalserver"
        fi
        openssl req -x509 -newkey rsa:2048 -nodes -keyout /etc/xrdp/key.pem -out /etc/xrdp/cert.pem -days 365 -subj "$CERTIFICATE_SUBJECT"
        crudini --set /etc/xrdp/xrdp.ini Globals security_layer tls
        crudini --set /etc/xrdp/xrdp.ini Globals certificate /etc/xrdp/cert.pem
        crudini --set /etc/xrdp/xrdp.ini Globals key_file /etc/xrdp/key.pem
    fi
    chown xrdp /etc/xrdp/rsakeys.ini /etc/xrdp/cert.pem /etc/xrdp/key.pem
- path: /default-desktop.tar.gz
  permissions: '0644'
  encoding: b64
  content: |
    
- path: /home/demo/.xsession
  permissions: '0644'
  content: |
    startxfce4
runcmd:
  - curl -s https://packages.cloud.google.com/apt/doc/apt-key.gpg | apt-key add -; touch /etc/apt/sources.list.d/kubernetes.list; echo "deb http://apt.kubernetes.io/ kubernetes-xenial main" | tee -a /etc/apt/sources.list.d/kubernetes.list; apt update; apt install -y kubectl
  - cd /usr/src; curl -fsSL -o get_helm.sh https://raw.githubusercontent.com/helm/helm/master/scripts/get-helm-3; chmod +x /usr/src/get_helm.sh; /usr/src/get_helm.sh
  - cd /usr/src; wget -c https://golang.org/dl/go1.17.linux-amd64.tar.gz -O - | tar -xz -C /usr/local; ln -s /usr/local/go/bin/go /usr/bin/go
  - curl -fsSL https://apt.releases.hashicorp.com/gpg | apt-key add -; apt-add-repository "deb [arch=$(dpkg --print-architecture)] https://apt.releases.hashicorp.com $(lsb_release -cs) main"; apt update; apt install terraform
  - cd /usr/src; wget https://dl.google.com/linux/direct/google-chrome-stable_current_amd64.deb; dpkg -i /usr/src/google-chrome-stable_current_amd64.deb
  - cd /usr/src; wget -q https://packages.microsoft.com/keys/microsoft.asc -O- | sudo apt-key add -; add-apt-repository -y "deb [arch=amd64] https://packages.microsoft.com/repos/vscode stable main"; apt update; apt -y install code
  - convert -background black -fill white -font Liberation-Sans -size 1920x1080 -gravity center -pointsize 128 label:`hostname` /usr/share/backgrounds/xfce/hostname.png
  - apt purge -y light-locker xscreensaver gsfonts; apt autoremove -y; rm -rf /var/cache/apt /var/lib/apt/lists
  - mkdir -p /home/demo/.ssh; cp /root/.ssh/authorized_keys /home/demo/.ssh/authorized_keys; chmod 0600 /home/demo/.ssh/authorized_keys; 
  - cp /etc/skel/.* /home/demo/
  - /usr/bin/xrdp-reset-certs
  - cd /; tar -xvzf default-desktop.tar.gz 
  - chown -R demo:demo /home/demo
  - ufw allow 3389
  - reboot
